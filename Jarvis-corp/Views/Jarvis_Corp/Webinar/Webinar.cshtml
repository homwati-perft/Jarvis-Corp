@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{
    Sitecore.Data.Fields.LinkField webinar_link = Model.Item.Fields["Link"];
}
<section class="panel-component insights-webinars" style="">
    <span>
        @Html.Sitecore().Field("Background")
        <div class="container" style="margin-top:-50%">
            <div class="row">
                <div class="col-xs-12 section-header">
                    <h2>
                        @Html.Sitecore().Field("Heading")
                    </h2>
                    <aside>
                        <a href="@webinar_link.Url" class="section-header-button">@Html.Sitecore().Field("Link Heading")</a>
                    </aside>
                </div>
            </div>
            <div class="row panels-row">
                @Html.Sitecore().DynamicPlaceholder("Registration")
            </div>
    </span>
</section>
