@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{
    Sitecore.Data.Fields.MultilistField partner_list = Model.Item.Fields["Partner List"];
}
<section class="cta-columns">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 section-header">

                <h2 class="valued-partner-title">@Html.Sitecore().Field("Heading")</h2>
                <aside>
                    @Html.Sitecore().Field("Sub-Heading")
                </aside>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12 column-block">
                <ul>
                    @foreach (Sitecore.Data.Items.Item partner in partner_list.GetItems())
                    {
                        Sitecore.Data.Fields.LinkField partner_link = partner.Fields["Link"];
                        <li><a href='@partner_link.Url' target='_blank'>@Html.Sitecore().Field("Link Heading",partner)</a></li>
                    }
                </ul>
            </div>
        </div>
    </div>
</section>