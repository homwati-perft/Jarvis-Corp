@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{
    Sitecore.Data.Fields.MultilistField table_data = Model.Item.Fields["Table List"];
}
<div class="panel-panel-inner">
    <div class="block-region-sidebar">
        <div data-storage-id="node:5806:full:27026" class="block--market-data-block__stock-quote block--market-data-block__stock-quote--5806 block--sidebar--market-data-block__stock-quote block--sidebar--market-data-block__stock-quote--5806 block--ef6147b4-5c48-4bb5-a249-efadc198f426 block--ef6147b4-5c48-4bb5-a249-efadc198f426--5806 block block-nir-market-data-block block-market-data-block__stock-quote">

            <h2>@Html.Sitecore().Field("Heading")</h2>

            <div class='quote-wrap user-toggle-off stock-quote__featured'>
                <div class="quote-wrapper">
                    <table width="100%" cellspacing="0" cellpadding="0" border="0">
                        <tbody>
                            <tr class="ccbnOutline">
                                <td valign="top">
                                    <table class="ccbnBgRow" width="100%" cellspacing="1" cellpadding="3" border="0">
                                        <form method="Post" action="phoenix.zhtml?c=83872&amp;p=irol-irhome&amp;t=SwitchQuote"></form>
                                        <tbody>
                                            <tr class="ccbnBgTblTtl ccbnBgTblEven">
                                                <td colspan="2">
                                                    <span class="ccbnTblTtl">
                                                        <input name="control_Symbol" value="03NA000000CAB" type="hidden"> @Html.Sitecore().Field("Investor Table Heading")
                                                    </span>
                                                </td>
                                            </tr>

                                            @foreach (Sitecore.Data.Items.Item table in table_data.GetItems())
                                            {
                                                <tr class="ccbnBgTblTxt ccbnBgTblEven">
                                                    <td class="ccbnBgTblLabelLeft" valign="top" nowrap="nowrap">
                                                        <span class="ccbnTblLabelLeft">
                                                            @Html.Sitecore().Field("Row Title",table)
                                                        </span>
                                                    </td>
                                                    <td valign="top" nowrap="nowrap">
                                                        <span class="ccbnTblTxt">
                                                            @Html.Sitecore().Field("Row Value", table)
                                                        </span>
                                                    </td>
                                                </tr>
                                            }
                                            <tr class="ccbnBgTblSubTxt">
                                                <td colspan="2"><span class="ccbnTblSubTxt">@Html.Sitecore().Field("Table Data Time")</span></td>
                                            </tr>


                                        </tbody>
                                    </table>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <div class="views-element-container block--views-blockwidget-assets-widget-assets-ul block--views-blockwidget-assets-widget-assets-ul--5806 block--sidebar--views-block--widget-assets-widget-assets-ul block--sidebar--views-block--widget-assets-widget-assets-ul--5806 block--1b406367-b045-4685-b050-c52a6dcde011 block--1b406367-b045-4685-b050-c52a6dcde011--5806 block block-views block-views-blockwidget-assets-widget-assets-ul">


            <div data-block-uuid="1b406367-b045-4685-b050-c52a6dcde011">
                <div class="view view-widget-assets view-id-widget_assets view-display-id-widget_assets_ul js-view-dom-id-c66c617ce1d15e247b2a542c775b1de5aaf86cf0745c531091bda65297353edf">
                    <div class="view-content">
                        <div class="item-list">
                            @{ 
                                Sitecore.Data.Items.Item pdfs = Sitecore.Context.Database.GetItem("{D6A14B1B-608A-478F-9421-5E1D651F1307}");
                                Sitecore.Collections.ChildList pdf_list = pdfs.GetChildren();
                            }
                            <ul>
                                @foreach (Sitecore.Data.Items.Item all_pdf in pdf_list)
                                {
                                    Sitecore.Data.Fields.FileField link = all_pdf.Fields["PDF"];
                                    string file_url = Sitecore.Resources.Media.MediaManager.GetMediaUrl(link.MediaItem);
                                    <li>
                                        <article role="article" class="node node--nir-asset--nir-unordered-list node--type-nir-asset node--view-mode-nir-unordered-list two-col-node-alt3">
                                            <div>
                                                <div class="nir-docs">
                                                    <div class="field field--name-field-nir-document field--type-file field--label-hidden field__items">
                                                        <div class="field__item">
                                                            <div class="file-link">
                                                                <span class="file file--mime-application-pdf file--application-pdf"> <a href="@file_url" style="display: inline-flex;">
                                                                    <span>@Html.Sitecore().Field("Pdf Logo",all_pdf)</span>
                                                                    @Html.Sitecore().Field("Heading", all_pdf)
                                                                    </a>
                                                                </span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </article>
                                    </li>
                                }

                            </ul>

                        </div>

                    </div>
                </div>
            </div>

        </div>
    </div>
</div>